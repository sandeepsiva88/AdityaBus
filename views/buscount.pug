  body
    section#container
      // header start
      header.header.black-bg
        .sidebar-toggle-box
          .fa.fa-bars.tooltips(data-placement='right', data-original-title='Toggle Navigation')
        // logo start
        a.logo(href='#')
          b
            img(src='images/myhub_logo.png',style='height:55px;width:100px;margin-top:-13px')
        // logo end
        .top-menu
          ul.nav.pull-right.top-menu
            li
              a.logout(href='logout') Logout
      // header end
      // sidebar start
      aside
        #sidebar.nav-collapse
          // sidebar menu start
          ul#nav-accordion.sidebar-menu
            p.centered
              a(href='#')
                img.img-circle(src='images/bus.png', width='80')
            h5.centered 
              p #{user.Town} Bus Count 
            li.active
              a(data-toggle='tab', href='#home')
                i.fa.fa-home
                span Home
          // sidebar menu end
      // sidebar end
      // main content start
      section#main-content
        section.wrapper.site-min-height
          .row.mt
            .col-lg-12
              .tab-content
                #home.tab-pane.fade.in.active
                  .container(style='background-color: #eaeaea;width: 100%;height:100%;border-radius: 1px')  
                    br
                    br
                    center
                      .row
                        .well
                          #printDate.date
                          #printDay.date
                          #printTime.date
                    .row 
                      br   
                      .col-lg-12
                        .panel.panel-default
                          .panel-body
                            .table-responsive
                              table#table.dataTables-example.table.table-striped.table-bordered.table-hover
                                thead
                                  tr
                                    th(style="display:none") Time
                                    th Bus No
                                    th Strenth
                                tbody
                                  each details in data
                                    tr
                                      td.time(style="display:none") #{details.Intime}
                                      td.busno #{details.Code}
                                      td
                                        input.count(type="number", name="count", class="form-control")
    
                                    
                              button.btn.btn-info(type='button', data-toggle='modal', data-target='#myModal') add           
        #myModal.modal.fade
          .modal-dialog
            .modal-content
              // Modal Header
              .modal-header
                h4.modal-title Adding Busdetails
                button.close(type='button', data-dismiss='modal') Ã—
              // Modal body
              .modal-body
                form.form-horizontal(action='/Addingbus', method='post')
                  .form-group
                    label.control-label.col-md-2 Bus No:
                    .col-md-6
                      input.form-control(type='text', name='busno', placeholder='Enter Bus No.', required)
                  .form-group
                    label.control-label.col-md-2 count:
                    .col-md-6
                      input.form-control(type='text', name='count', required)
                  .form-group
                    .col-sm-offset-2.col-sm-10
                      input.btn.btn-info(type='submit', value='submit')                                           
